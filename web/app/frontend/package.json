{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend for the HireMe auction",
  "private": true,
  "scripts": {
    "build": "nwb build-preact-app",
    "clean": "nwb clean-app",
    "start": "nwb serve-preact-app",
    "test": "nwb test-preact",
    "test:coverage": "nwb test-preact --coverage",
    "test:watch": "nwb test-preact --server"
  },
  "eslintConfig": {
    "extends": "standard-preact",
    "rules": {
      "jsx-quotes": [
        "error",
        "prefer-double"
      ]
    }
  },
  "dependencies": {
    "@ibm/type": "^0.5.4",
    "bluebird": "^3.5.1",
    "countdown": "^2.6.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "purecss": "^1.0.0",
    "truffle-contract": "^3.0.3",
    "web3": "^1.0.0-beta.30"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-standard-preact": "^1.1.6",
    "nwb": "0.21.x",
    "nwb-less": "^0.6.1",
    "preact-router": "^2.6.0"
  },
  "author": "Koh Wei Jie",
  "license": "MIT",
  "repository": "https://github.com/weijiekoh/hireme"
}
